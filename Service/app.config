<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="net.vieapps.repositories" type="net.vieapps.Components.Utility.AppConfigurationSectionHandler, VIEApps.Components.Utility" />
		<section name="dbProviderFactories" type="net.vieapps.Components.Utility.AppConfigurationSectionHandler, VIEApps.Components.Utility" />
		<section name="memcached" type="Enyim.Caching.Configuration.MemcachedClientConfigurationSectionHandler, Enyim.Caching" />
	</configSections>
	<appSettings>
		<add key="vieapps:LogsPath" value="\\Mac\Home\Documents\VIE Apps\log-files" />
		<add key="vieapps:HttpFilesUri" value="http://local-afs.vieapps.net" />
	</appSettings>
	<connectionStrings>
		<add name="NoSQL" providerName="MongoDB" connectionString="mongodb://sa:sa%4012345@24.4.77.2/admin" />
		<add name="SQL" providerName="MySql.Data.MySqlClient" connectionString="server=24.4.77.2;database=VIEApps-Files;uid=root;pwd=sa@12345;max pool size=20;min pool size=1;" />
		<!--add name="SQL" providerName="System.Data.SqlClient" connectionString="server=24.4.77.3;database=VIEApps-Books;uid=sa;pwd=sa@12345;max pool size=20;min pool size=1;" /-->
	</connectionStrings>
	<net.vieapps.repositories ensureSchemas="true" ensureIndexes="true">
		<repository type="net.vieapps.Services.Files.Repository`1, VIEApps.Services.Files.Component" primaryDataSource="NoSQL" secondaryDataSource="SQL" syncDataSources="" />
		<dataSources>
			<dataSource name="NoSQL" mode="NoSQL" connectionStringName="NoSQL" databaseName="VIEApps-Files" />
			<dataSource name="SQL" mode="SQL" connectionStringName="SQL" />
		</dataSources>
	</net.vieapps.repositories>
	<dbProviderFactories>
		<add invariant="MySql.Data.MySqlClient" type="MySql.Data.MySqlClient.MySqlClientFactory, MySqlConnector" name="MySQL Provider" description=".NET Framework Data Provider for MySQL" />
		<add invariant="System.Data.SqlClient" type="System.Data.SqlClient.SqlClientFactory, System.Data.SqlClient" name="SQL Server Provider" description=".NET Framework Data Provider for SQL Server" />
	</dbProviderFactories>
	<memcached>
		<servers>
			<add address="24.4.77.2" port="16429" />
		</servers>
		<socketPool minPoolSize="10" maxPoolSize="100" connectionTimeout="00:00:05" deadTimeout="00:01:00" />
	</memcached>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1" />
	</startup>
</configuration>
