		</sectionGroup>
	</configSections>
	<enyim.com>
		<memcached>
			<servers>
				<add address="24.4.77.2" port="16429" />
				<add address="24.4.77.3" port="16429" />
			</servers>
			<socketPool minPoolSize="10" maxPoolSize="100" connectionTimeout="00:00:05" deadTimeout="00:01:00" />
		</memcached>
	</enyim.com>
	<net.vieapps.files.handlers>
		<handler key="avatars" type="net.vieapps.Services.Files.AvatarHandler, VIEApps.Services.Files" />
		<handler key="captchas" type="net.vieapps.Services.Files.CaptchaHandler, VIEApps.Services.Files" />
		<handler key="thumbnails" type="net.vieapps.Services.Files.ThumbnailHandler, VIEApps.Services.Files" />
		<handler key="thumbnailpngs" type="net.vieapps.Services.Files.ThumbnailHandler, VIEApps.Services.Files" />
		<handler key="thumbnailbigs" type="net.vieapps.Services.Files.ThumbnailHandler, VIEApps.Services.Files" />
		<handler key="thumbnailbigpngs" type="net.vieapps.Services.Files.ThumbnailHandler, VIEApps.Services.Files" />
		<handler key="files" type="net.vieapps.Services.Files.FileHandler, VIEApps.Services.Files" />
		<handler key="downloads" type="net.vieapps.Services.Files.DownloadHandler, VIEApps.Services.Files" />
	</net.vieapps.files.handlers>
	<appSettings>
		<add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
		<add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
		<add key="aspnet:RestrictXmlControls" value="true" />
		<add key="vieapps:BypassSegments" value="favicon.ico|__browserLink" />
		<add key="vieapps:HiddenSegments" value="" />
		<add key="vieapps:StaticSegments" value=".well-known" />
		<add key="vieapps:LogsPath" value="\\Mac\Home\Documents\VIE Apps\logs" />
		<add key="vieapps:UserAvatarFilesPath" value="\\Mac\Home\Documents\VIE Apps\data-files\user-avatars" />
		<add key="vieapps:DefaultUserAvatarFileName" value="@default.png" />
		<add key="vieapps:AttachmentFilesPath" value="\\Mac\Home\Documents\VIE Apps\data-files\attachments" />
		<add key="vieapps:PassportUrl" value="http://local-apis-users.vieapps.net" />
	</appSettings>
	<system.web>
		<customErrors mode="Off" />
		<trace enabled="false" />
		<healthMonitoring>
			<rules>
				<remove name="Failure Audits Default" />
			</rules>
		</healthMonitoring>
		<sessionState mode="Custom" cookieless="UseCookies" cookieName=".VIEApps-Session-ID" regenerateExpiredSessionId="true" customProvider="FilesSessionStateProvider">
