				<add name="FilesSessionStateProvider" type="net.vieapps.Components.Caching.Web.SessionStateProvider, VIEApps.Components.Caching" />
			</providers>
		</sessionState>
		<authentication mode="Forms">
			<forms name=".VIEApps-Files" protection="All" timeout="5" cookieless="UseCookies" path="/" slidingExpiration="false" />
		</authentication>
		<compilation debug="true" targetFramework="4.7" batch="false" optimizeCompilations="true" />
		<httpRuntime targetFramework="4.7" executionTimeout="90" maxRequestLength="819200" useFullyQualifiedRedirectUrl="false" minFreeThreads="8" minLocalRequestFreeThreads="4" appRequestQueueLimit="65535" requestValidationMode="2.0" enableVersionHeader="false" maxUrlLength="2048" maxQueryStringLength="10240" />
		<machineKey validationKey="BBDC2FB3316E50C0347143B3ACB0A51E49672C45117DAB8F4C616B6870BB090E30EB61D0F55A8ED66B44EC56778384003990EE347AC942C2E05AE413DB7F7B35" decryptionKey="B4560E2A5B5D5F79D125D19124F61FAD6318B184CC09C85A5917CE902022F38F" validation="SHA1" decryption="AES" compatibilityMode="Framework45" />
	</system.web>
	<system.webServer>
		<modules runAllManagedModulesForAllRequests="true" />
		<handlers>
			<remove name="SimpleHandlerFactory-Integrated-4.0" />
			<add name="SimpleHandlerFactory-Integrated-4.0" path="*.ashx" verb="HEAD,GET,POST,OPTIONS" type="System.Web.UI.SimpleHandlerFactory" preCondition="integratedMode,runtimeVersionv4.0" />
			<remove name="ExtensionlessUrlHandler-Integrated-4.0" />
			<add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="HEAD,GET,POST,OPTIONS" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
		</handlers>
		<httpErrors errorMode="Custom">
			<remove statusCode="403" subStatusCode="-1" />
			<error statusCode="403" path="/Global.ashx" prefixLanguageFilePath="" responseMode="ExecuteURL" />
			<remove statusCode="404" subStatusCode="-1" />
			<error statusCode="404" path="/Global.ashx" prefixLanguageFilePath="" responseMode="ExecuteURL" />
		</httpErrors>
		<httpProtocol>
			<customHeaders>
				<remove name="X-Powered-By" />
			</customHeaders>
		</httpProtocol>
		<security>
			<requestFiltering allowDoubleEscaping="true">
				<requestLimits maxAllowedContentLength="838860800" maxUrl="2048" maxQueryString="10240" />
			</requestFiltering>
		</security>
		<directoryBrowse enabled="false" />
	</system.webServer>
	<system.net>
		<connectionManagement>
			<add address="*" maxconnection="48" />
		</connectionManagement>
	</system.net>
